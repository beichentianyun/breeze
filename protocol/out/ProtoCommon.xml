<?xml version="1.0" encoding="UTF-8"?>

<ProtoTraits>
  <MinNo>0</MinNo>
  <MaxNo>0</MaxNo>
  <UseLog4z>0</UseLog4z>
</ProtoTraits>


<Proto>

  <enum name="ErrorCode" desc="错误类型">
    <member name="EC_SUCCESS" value="0"  desc="成功"/>
    <member name="EC_UNKOWN_ERROR" value="1"  desc="未知错误"/>
    <member name="EC_SERVER_ERROR"  value="2" desc="服务器内部错误"/>
    <member name="EC_AUTH_NOT_FOUND" value="3"  desc="认证错误:找不到用户名"/>
    <member name="EC_AUTH_ERROR"  value="4" desc="认证失败"/>
    <member name="EC_AUTH_ING"  value="5" desc="正在认证中..."/>
    <member name="EC_AUTH_DOUBLE"  value="6" desc="重复认证"/>
  </enum>


  <struct name="CharactorInfo" desc="角色信息">
    <member name="accountName" type="string"   desc=""/>
    <member name="accountID" type="ui64"   desc=""/>
    <member name="charName" type="string"   desc=""/>
    <member name="charID" type="ui64"   desc=""/>
    <member name="pwd"  type="string" desc=""/>
  </struct>

</Proto>

